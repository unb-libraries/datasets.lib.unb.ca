{
  "authors": [
    {
      "name": "UNB Libraries libsupport@unb.ca",
      "role": "Creator"
    }
  ],
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "drupal/core-composer-scaffold": true
    },
    "discard-changes": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "description": "Drupal 8 component of datasets.lib.unb.ca",
  "extra": {
    "enable-patching": true,
    "installer-paths": {
      "core": [
        "type:drupal-core"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "libraries/{$name}": [
        "type:drupal-library"
      ],
      "modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "themes/contrib/{$name}": [
        "type:drupal-theme"
      ]
    },
    "patches": {
      "drupal/core": {
        "To correct DerivativeDiscoveryDecorator warning": "https://www.drupal.org/files/issues/patch_empty_fix.patch"
      }
    }
  },
  "license": "MIT",
  "minimum-stability": "dev",
  "name": "unb-libraries/datasets.lib.unb.ca",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "package": {
        "dist": {
          "type": "file",
          "url": "https://malsup.github.io/jquery.cycle.all.js"
        },
        "name": "malsup/jquery.cycle",
        "require": {
          "composer/installers": "^1.0.20"
        },
        "type": "drupal-library",
        "version": "1.0"
      },
      "type": "package"
    },
    {
      "package": {
        "name": "drupal/login_return_page",
        "source": {
          "reference": "8.x-1.x",
          "type": "git",
          "url": "https://git.drupalcode.org/sandbox/gnuget-2824414.git"
        },
        "type": "drupal-module",
        "version": "1.x-dev"
      },
      "type": "package"
    }
  ],
  "require": {
    "drupal/admin_toolbar": "3.3.0",
    "drupal/bootstrap_barrio": "5.1.6",
    "drupal/core": "9.5.2",
    "drupal/externalauth": "1.4",
    "drupal/fontawesome": "^2.23",
    "drupal/google_analytics": "2.5",
    "drupal/health_check": "1.3",
    "drupal/login_return_page": "1.x-dev",
    "drupal/pathauto": "1.11",
    "drupal/redis": "1.6",
    "drupal/samlauth": "3.8",
    "oomphinc/composer-installers-extender": "^2",
    "unb-libraries/alert_scheduler": "dev-8.x-1.x",
    "unb-libraries/calendar_hours": "dev-9.x-1.x",
    "unb-libraries/drupal-builder": "^9",
    "unb-libraries/saml_features": "dev-9.x-1.x",
    "unb-libraries/unb_lib_theme": "dev-9.x-4.x"
  },
  "require-dev": {
    "unb-libraries/drupal-development-libraries": "^8.0.0"
  },
  "scripts": {
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "post-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "post-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "pre-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "pre-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ]
  },
  "type": "project"
}
