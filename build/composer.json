{
    "authors": [
        {
            "name": "UNB Libraries libsupport@unb.ca",
            "role": "Creator"
        }
    ],
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "description": "Drupal 8 component of datasets.lib.unb.ca",
    "extra": {
        "enable-patching": true,
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        },
        "patches": {
            "drupal/core": {
                "To correct DerivativeDiscoveryDecorator warning": "https://www.drupal.org/files/issues/patch_empty_fix.patch",
                "To display exposed filter block titles": "https://www.drupal.org/files/issues/2720101-38.patch"
            }
        }
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "name": "unb-libraries/datasets.lib.unb.ca-drupal8",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "package": {
                "dist": {
                    "type": "file",
                    "url": "https://malsup.github.io/jquery.cycle.all.js"
                },
                "name": "malsup/jquery.cycle",
                "require": {
                    "composer/installers": "^1.0.20"
                },
                "type": "drupal-library",
                "version": "1.0"
            },
            "type": "package"
        },
        {
            "type": "git",
            "url": "https://github.com/unb-libraries/drupal-development-libraries.git"
        },
        {
            "package": {
                "name": "drupal/login_return_page",
                "source": {
                    "reference": "8.x-1.x",
                    "type": "git",
                    "url": "https://git.drupal.org/sandbox/gnuget/2824414.git"
                },
                "type": "drupal-module",
                "version": "1.x-dev"
            },
            "type": "package"
        },
        {
            "type": "git",
            "url": "https://github.com/unb-libraries/unb_lib_theme.git"
        }
    ],
    "require": {
        "composer/installers": "^1.2.0",
        "cweagans/composer-patches": "1.6.3",
        "drupal-composer/drupal-scaffold": "~2.2.0",
        "drupal/bootstrap": "3.15",
        "drupal/config_update": "1.1",
        "drupal/console": "~1.0",
        "drupal/core": "8.6.3",
        "drupal/externalauth": "1.1",
        "drupal/features": "3.0-beta8",
        "drupal/google_analytics": "2.3",
        "drupal/ldap": "3.0-beta5",
        "drupal/login_return_page": "1.x-dev",
        "drupal/pathauto": "1.0-rc1",
        "drupal/search_api": "1.0",
        "drush/drush": "^8.1.15",
        "oomphinc/composer-installers-extender": "1.1.1",
        "unb-libraries/unb_lib_theme": "dev-8.x-bootstrap-3.x",
        "webflo/drupal-finder": "^0.3.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "unb-libraries/drupal-development-libraries": "^8.0.0"
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ]
    },
    "type": "project"
}
