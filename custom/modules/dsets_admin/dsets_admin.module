/**
* Implements hook_page_attachments().
*/
function dsets_admin_page_attachments(array &$attachments) {
 if (!\Drupal::currentUser()->hasPermission('access toolbar')) {
   return;
 }

 $attachments['#attached']['library'][] = 'dsets_admin/toolbar-overrides';
}

